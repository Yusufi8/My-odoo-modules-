<odoo>
    <record id="seq_student_roll" model="ir.sequence">
        <field name="name">Student Roll Number</field>
        <field name="code">college.student</field>
        <field name="prefix">STU</field>
        <field name="padding">4</field>
    </record>
    <record id="seq_course_code" model="ir.sequence">
        <field name="name">Course Code</field>
        <field name="code">college.course</field>
        <field name="prefix">CRS</field>
        <field name="padding">3</field>
    </record>
</odoo>
