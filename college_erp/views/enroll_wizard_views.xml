<odoo>
    <record id="view_enroll_wizard_form" model="ir.ui.view">
        <field name="name">college.enroll.wizard.form</field>
        <field name="model">college.enroll.wizard</field>
        <field name="arch" type="xml">
            <form string="Enroll Student">
                <group>
                    <field name="student_id"/>
                    <field name="course_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button string="Enroll" type="object" name="action_enroll" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
